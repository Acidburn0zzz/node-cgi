#!/usr/bin/env node
/*jshint node: true */
'use strict';


var Processor = require('../lib/Processor'),
	filename = process.env.PATH_TRANSLATED,
	options = {
		defaultType: 'js',
		typeByExt: {
			'.jade.nd': 'jade',
			'.html.nd': 'html'
		}
	};


Processor.processFile(filename, options).toStdout();
